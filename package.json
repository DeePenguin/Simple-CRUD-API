{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "src/index.ts",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "format": "prettier --write ./src",
    "build": "webpack",
    "start:dev": "tsnd --env-file=.env --respawn ./src",
    "start:prod": "npm run build && node --env-file .env dist",
    "start:dev:multi": "npm run start:dev -- --multi=true",
    "start:prod:multi": "npm run start:prod -- --multi=true"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/DeePenguin/Simple-CRUD-API.git"
  },
  "author": "Dee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DeePenguin/Simple-CRUD-API/issues"
  },
  "homepage": "https://github.com/DeePenguin/Simple-CRUD-API#readme",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-webpack-plugin": "^4.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  }
}
